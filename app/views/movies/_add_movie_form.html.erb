
<!-- Adding movie & setting date form-->
<%= form_with model: @movies, local:true, url: '/add_movie' do |form|%>
<%= hidden_field_tag(:user_id, @currentUser)%>
<%= hidden_field_tag(:movie_id, @movie_id)%>
<%= hidden_field_tag(:movietitle, @response["Title"] )%>
<%= hidden_field_tag(:moiveposter, @response["Poster"]) %>
<%= form.date_field(:setdate)%>
<%= form.submit "Submit"%>
<% end%>